    "yaml-eslint-parser": "^1.1.0"
  },
  "resolutions": {
    "eslint-plugin-import": "npm:eslint-plugin-i@^2.26.0-2"
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  },
  "lint-staged": {
    "{src,__tests__}/**/*.js": "eslint --cache --fix",
    "*.ts": [
